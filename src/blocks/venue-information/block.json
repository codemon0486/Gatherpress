{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "gatherpress/venue-information",
	"version": "0.2",
	"title": "Venue Information",
	"category": "gatherpress",
	"icon": "location",
	"example": {},
	"description": "The venue information block.",
	"attributes": {
		"blockId": {
			"type": "string"
		},
		"encodedAddressURL": {
			"type": "string",
			"default": ""
		},
		"phoneNumber": {
			"type": "string",
			"default": ""
		},
		"website": {
			"type": "string",
			"default": ""
		},
		"mapId": {
			"type": "string"
		},
		"align": {
			"type": "string",
			"default": ""
		},
		"fullAddress": {
			"type": "string",
			"default": "currentLocation"
		},
		"zoom": {
			"type": "number",
			"default": 10
		},
		"type": {
			"type": "string",
			"default": "m"
		},
		"deskHeight": {
			"type": "number",
			"default": 400
		},
		"tabHeight": {
			"type": "number",
			"default": 300
		},
		"mobileHeight": {
			"type": "number",
			"default": 250
		},
		"device": {
			"type": "string",
			"default": "desktop"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "gatherpress",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
